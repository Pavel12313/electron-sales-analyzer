{
  "name": "bms-sales-analyzer",
  "version": "1.0.0",
  "description": "BMS app to analyze Google Sheets sales data",
  "main": "dist/main.js",
  "scripts": {
  "start": "cross-env NODE_ENV=development concurrently \"npm run start:renderer\" \"wait-on http://localhost:3000 && npm run start:electron\"",
  "start:renderer": "webpack serve --config webpack.renderer.config.js",
  "start:electron": "electron .",
  "build": "webpack --config webpack.main.config.js && webpack --config webpack.renderer.config.js",
  "package": "npm run build && electron-builder --mac --win"
},
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@emotion/react": "^11.10.6",
    "@emotion/styled": "^11.10.6",
    "@google-cloud/local-auth": "^2.1.1",
    "@mui/material": "^5.11.14",
    "chart.js": "^3.9.1",
    "googleapis": "^105.0.0",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.3.1",
    "react-dom": "^17.0.2",
    "react-dropzone": "^14.2.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "babel-loader": "^9.2.1",
    "concurrently": "^7.6.0",
    "copy-webpack-plugin": "^11.0.0",
    "cross-env": "^7.0.3",
    "electron": "^22.0.0",
    "electron-builder": "^23.6.0",
    "electron-reload": "^2.0.0-alpha.1",
    "html-webpack-plugin": "^5.5.0",
    "react-refresh": "^0.14.2",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "wait-on": "^7.2.0",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.2"
  }
}
